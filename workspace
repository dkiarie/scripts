tmux has-session -t workspace
if [ $? != 0 ]
then
  tmux new-session -s workspace -n servers -d
  tmux send-keys -t workspace 'cd ~/code' C-m
  tmux new-window -n editor -t workspace
  tmux send-keys -t workspace:2 'cd ~/code' C-m
  tmux new-window -n tests -t workspace
  tmux send-keys -t workspace:3 'cd ~/code' C-m
  tmux new-window -n notes -t workspace
  tmux send-keys -t workspace:4 'cd ~/notes' C-m
  tmux send-keys -t workspace:4 'e .' C-m
  tmux select-window -t workspace:1
  tmux send-keys -t workspace:1 'weather' C-m
fi
tmux attach -t workspace
